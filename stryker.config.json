{
	"$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
	"checkers": [
		"typescript"
	],
	"coverageAnalysis": "perTest",
	"ignorers": [
		"constructor",
		"this.logger"
	],
	"ignoreStatic": true,
	"incremental": true,
	"jest": {},
	"mutate": [
		"src/**/*.ts",
		"src/**/*.spec.ts",
		"src/main.ts",
		"src/**/*.config.ts",
		"src/**/*.module.ts"
	],
	"mutator": {
		"excludedMutations": [
			"OptionalChaining"
		]
	},
	"plugins": [
		"./stryker-ignorer.mjs",
		"@stryker-mutator/*"
	],
	"reporters": [
		"clear-text",
		"html",
		"json",
		"progress"
	],
	"testRunner": "jest",
	"timeoutMS": 5000,
	"tsconfigFile": "tsconfig.json",
	"typescriptChecker": {
		"prioritizePerformanceOverAccuracy": true
	}
}
